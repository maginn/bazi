# frontend/.env
# React前端环境配置文件
# 访问地址: http://localhost:3000

# 前端开发服务器端口
PORT=3000

# 后端API基础URL
REACT_APP_API_BASE_URL=http://localhost:5001/api

# 应用信息
REACT_APP_NAME=八字命理系统
REACT_APP_VERSION=1.0.0

# 开发模式设置
GENERATE_SOURCEMAP=true

# 禁用ESLint检查（可选）
ESLINT_NO_DEV_ERRORS=true

# 自动打开浏览器
BROWSER=default